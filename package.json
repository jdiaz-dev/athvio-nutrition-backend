{
  "name": "athvio-nutrition-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "dotenvx run -f .env.development -- nest start",
    "start:dev": "chmod +x nest-start-dev.sh && ./nest-start-dev.sh",
    "start:debug": "dotenvx run -f .env.development -- nest start --debug --watch",
    "start:prod": "dotenvx run -f .env.production -f .env -- node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "cli:save:mongodb": "dotenvx run -f .env.development -- ts-node ./scripts/save-mongodb-records.ts",
    "cli:populatedb": "dotenvx run -f .env.development -- ts-node -r tsconfig-paths/register src/cli/cli.ts populatedb",
    "nsolid:test": "dotenvx run -f .env.development -- nsolid dist/main.js"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@as-integrations/fastify": "^3.1.0",
    "@aws-sdk/client-s3": "^3.821.0",
    "@aws-sdk/client-ses": "^3.821.0",
    "@aws-sdk/client-sesv2": "^3.821.0",
    "@dotenvx/dotenvx": "^1.44.1",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
    "@nestjs/apollo": "^13.2.1",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.1.2",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/graphql": "^13.2.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.9",
    "@nestjs/throttler": "^6.4.0",
    "@polar-sh/sdk": "^0.35.4",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dayjs": "^1.11.13",
    "deepl-node": "^1.18.0",
    "google-auth-library": "^10.2.1",
